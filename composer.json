{
  "name": "guysartorelli/extended-ddev",
  "description": "A VERY OPINIONATED local development CLI wrapped around DDEV",
  "license": "BSD-3-Clause",
  "bin": [
    "bin/eddev"
  ],
  "repositories": {
    "guysartorelli/ddev-wrapper": {
      "type": "vcs",
      "url": "git@github.com:GuySartorelli/ddev-wrapper.git"
    }
  },
  "require": {
    "php": "^8.1",
    "guysartorelli/ddev-wrapper": "dev-main",
    "symfony/console": "^6.0",
    "symfony/process": "^6.0",
    "symfony/dotenv": "^6.0",
    "symfony/filesystem": "^6.3",
    "knplabs/github-api": "^3.13",
    "symfony/http-client": "^6.4",
    "nyholm/psr7": "^1.8",
    "knplabs/packagist-api": "^2.0",
    "composer/semver": "^3.4",
    "gitonomy/gitlib": "^1.3"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "GuySartorelli\\ExtendedDdev\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GuySartorelli\\ExtendedDdev\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
